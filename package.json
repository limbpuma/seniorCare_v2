{
  "name": "seniorcare",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.20.8 <19 || >=20.3.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "lint": "eslint .",
    "prettier": "prettier -w .",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:nav": "astro dev & sleep 5 && tsx src/utils/test-navigation.ts; exit_code=$?; pkill -f 'astro dev'; exit $exit_code",
